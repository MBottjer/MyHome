<% if current_user %>

  <div class="row">

    <div class="small-2 large-4 columns">
      <%= render "myhome/new_artist" %>
      <% @videos.each do |video| %>
      
        <a class="show-video">
          <p>
        <%=truncate(video.videos.first.title, :length => 40)%>
          </p>
        </a>
      
        <div class="video" style="display:none">
        <%= raw video.videos.first.embed_html %>
        </div>
      <% end %>
    </div>
    <div class="small-4 large-4 columns">
      <ul class="nav nav-pills nav-stacked">
      <% @links.each do |link| %>
        <div class="breakers" style="padding-top: 0px;">
          <li><a href="<%= link.uri %>" class="<%= link.relevant_icon %>"><%= link.name %></a><span class="editButton">&nbsp;&nbsp;
            <%= link_to(edit_user_link_path(link.id, current_user.id)) do %>
              <i class="fa fa-cog"></i>
            <% end %>&nbsp;&nbsp;
            <%= link_to(user_link_path(@user.id, link), method: :delete) do %>
              <i class="fa fa-times-circle fa-1x"></i>
            <% end %>
        </div>
      <% end %>
      </ul>

    </div>
    
    <div class="small-6 large-4 columns">

      <div class="colour_bar">
        <span class="javascript" style="width:<%= (Github.repos_by_language_percentage(@languages)["JavaScript"]).to_s + "%" %>;"></span><span class="ruby" style="width:<%= (Github.repos_by_language_percentage(@languages)["Ruby"]).to_s + "%" %>;"></span><span class="css" style="width:<%= (Github.repos_by_language_percentage(@languages)["CSS"]).to_s + "%" %>;"></span><span class="nil" style="width:<%= (Github.repos_by_language_percentage(@languages)[nil]).to_s + "%" %>;"></span>
      </div>
      <div class="color-scheme"><div class="javascript_box"></div><span class="key">JavaScript</span>&nbsp;<div class="nil_box"></div><span class="key">nil</span>&nbsp;<div class="ruby_box"></div><span class="key">Ruby</span>&nbsp;<div class="css_box"></div><span class="key">CSS</span></div>

      <div id="repos">
        <ul id="repo_list">
        </ul>
      </div>
      <div id="repo_content"></div>
      <button id="my_repos">My Repos</button><span id="repo_count"></span>
    </div>

  </div>

  
 
 
  
<% else %> 

  <div class="row">
    <div class="small-6 large-centered columns middle">
      Hello, nothing to see here.
    </div>
  </div>


<% end %>



