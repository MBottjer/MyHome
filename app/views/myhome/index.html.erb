<% if current_user %>

  <div class="row">

    <div class="small-2 large-4 columns">
      <%= render "myhome/new_artist" %>
      <% @videos.each do |video| %>
        <a class="show-video">
        <%=truncate(video.videos.first.title, :length => 40)%>
        </a>
        <div class="video" style="display:none">
        <%= raw video.videos.first.embed_html %>
        </div>
      <% end %>
    </div>
    <div class="small-4 large-4 columns">
      <ul class="nav nav-pills nav-stacked">
      <% @links.each do |link| %>
        <div class="breakers" style="padding-top: 0px;">
          <li><a href="<%= link.uri %>" class="<%= link.has_relevant_icon %>"><%= link.name %></a><span class="editButton">&nbsp;&nbsp;
            <%= link_to(edit_user_link_path(link.id, current_user.id)) do %>
              <i class="fa fa-cog"></i>
            <% end %>&nbsp;&nbsp;
            <%= link_to(user_link_path(@user.id, link), method: :delete) do %>
              <i class="fa fa-times-circle fa-1x"></i>
            <% end %>
        </div>
      <% end %>
      </ul>

    </div>
    
    <div class="small-6 large-4 columns"><%= @information %></div>
  </div>

  
 
 
  
<% else %> 

  <div class="row">
    <div class="small-6 large-centered columns middle">
      Hello, nothing to see here.
    </div>
  </div>


<% end %>



